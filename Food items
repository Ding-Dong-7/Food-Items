#include<stdio.h>
int main()
{
  int num;
  printf("Enter a random number from 1 to 5:");
  scanf("%c",&num);
  switch(num)
  {
    case'1':
    printf("Food item- Pizza\n Price- Rs 200");
    break;
    case'2':
    printf("Food item- French Fries\n Price- Rs 99");
    break;
    case'3':
    printf("Food Item- Pasta\n Price- Rs 190");
    break;
    case'4':
    printf("Food Item- Tteokbokki\n Price- Rs 390");
    break;
    case'5':
    printf("Food Item- Ramen\n Price- Rs 290");
    break;
    default:
    printf("Not valid");
  }
  return 0;
}
